<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MyUtils">

    <SolidColorBrush Color="#D32F2F" x:Key="Red" />
    <SolidColorBrush Color="#7B1FA2" x:Key="Purple" />
    <SolidColorBrush Color="#1976D2" x:Key="Blue" />
    <SolidColorBrush Color="#00796B" x:Key="Teal" />
    <SolidColorBrush Color="#388E3C" x:Key="Green" />
    <SolidColorBrush Color="#FBC02D" x:Key="Yellow" />
    <SolidColorBrush Color="#F57C00" x:Key="Orange" />
    <SolidColorBrush Color="#5D4037" x:Key="Brown" />
    <SolidColorBrush Color="#000000" x:Key="Black" />
    <SolidColorBrush Color="#616161" x:Key="Grey" />
    <SolidColorBrush Color="#FFFFFF" x:Key="White" />

    <x:Array Type="{x:Type Color}" x:Key="Colors">
        <Color>#D32F2F</Color>
        <Color>#7B1FA2</Color>
        <Color>#1976D2</Color>
        <Color>#00796B</Color>
        <Color>#388E3C</Color>
        <Color>#FBC02D</Color>
        <Color>#F57C00</Color>
        <Color>#5D4037</Color>
        <Color>#000000</Color>
        <Color>#616161</Color>
        <Color>#FFFFFF</Color>
    </x:Array>


</ResourceDictionary>